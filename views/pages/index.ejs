<head>
  <%- include ('../partials/head') %>
</head>

<body>
  <%- include ('../partials/header') %>

    <div class="container mx-auto py-10 px-4 lg:px-0">
      <div class="grid grid-cols-2 sm:grid-cols-3 md:grid-cols-5 xl:grid-cols-8 gap-4">
        <% for(let item of data) {%>
          <div>
            <div class="relative h-40 border rounded shadow text-center overflow-hidden">
              <a title="<%= item.title %>" href="<%= item.url %>" alt="<%= item.title %>">
                <img src="<%= item.image %>" class="object-cover w-full" />
              </a>
            </div>
            <a href="<%= item.url %>" class="text-black hover:text-blue-500 text-sm">
              <%= item.title %>
            </a>
          </div>
          <% } %>
      </div>

    </div>
</body>

</html>